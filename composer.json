{
    "name": "panlatent/craft-enums",
    "type": "yii2-extension",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "panlatent\\craft\\enums\\": "src/"
        }
    },
    "extra": {
        "bootstrap": "panlatent\\craft\\enums\\Extension"
    },
    "authors": [
        {
            "name": "panlatent",
            "email": "panlatent@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "craftcms/cms": "^4.0|^5.0",
        "craftcms/generator": "^1.3|^2.0"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
